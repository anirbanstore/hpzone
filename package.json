{
  "name": "hpzone",
  "author": "anirbmuk",
  "description": "Angular 12 application for managing my HP gas delivery schedule and status",
  "version": "6.0.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anirbanstore/hpzone.git"
  },
  "bugs": {
    "url": "https://github.com/anirbanstore/hpzone/issues"
  },
  "homepage": "https://hpzone-home.firebaseapp.com",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "test-ci": "ng test --watch=false",
    "build-prod": "ng build --configuration production",
    "pretty-check": "prettier --config ./.prettierrc --list-different \"src/app/**/*{.ts,.json,.css,.html}\"",
    "pretty-fix": "prettier --config ./.prettierrc --write \"src/app/**/*{.ts,.json,.css,.html}\"",
    "deploy": "npm run test-ci && npm run lint && npm run pretty-check && npm run build-prod && firebase deploy"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.0.1",
    "@angular/common": "~12.0.1",
    "@angular/compiler": "~12.0.1",
    "@angular/core": "~12.0.1",
    "@angular/forms": "~12.0.1",
    "@angular/platform-browser": "~12.0.1",
    "@angular/platform-browser-dynamic": "~12.0.1",
    "@angular/platform-server": "~12.0.1",
    "@angular/router": "~12.0.1",
    "@angular/service-worker": "~12.0.1",
    "@ngneat/until-destroy": "^8.0.4",
    "@ngrx/effects": "^12.0.0",
    "@ngrx/store": "^12.0.0",
    "@ngrx/store-devtools": "^12.0.0",
    "bootstrap": "^3.4.1",
    "ngx-spinner": "^11.0.1",
    "rxjs": "~6.5.4",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.0.1",
    "@angular/cli": "~12.0.1",
    "@angular/compiler-cli": "~12.0.1",
    "@nguniversal/builders": "^11.2.1",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "prettier": "^2.2.1",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "tslint-etc": "^1.13.9",
    "typescript": "~4.2.4"
  }
}