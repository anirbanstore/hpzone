{
  "name": "hpzone",
  "author": "anirbmuk",
  "description": "Angular application for managing my HP gas delivery schedule and status",
  "version": "8.0.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anirbanstore/hpzone.git"
  },
  "bugs": {
    "url": "https://github.com/anirbanstore/hpzone/issues"
  },
  "homepage": "https://hpzone-home.firebaseapp.com",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "clean": "rimraf .firebase && rimraf dist",
    "build:prod": "ng build --configuration production",
    "format:check": "prettier --config ./.prettierrc --list-different \"src/app/**/*{.ts,.json,.css,.html}\"",
    "format:fix": "prettier --config ./.prettierrc --write \"src/app/**/*{.ts,.json,.css,.html}\"",
    "deploy": "npm run lint && npm run format:check && npm run build:prod && firebase deploy && npm run clean"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.3.0",
    "@angular/common": "~13.3.0",
    "@angular/compiler": "~13.3.0",
    "@angular/core": "~13.3.0",
    "@angular/forms": "~13.3.0",
    "@angular/platform-browser": "~13.3.0",
    "@angular/platform-browser-dynamic": "~13.3.0",
    "@angular/platform-server": "~13.3.0",
    "@angular/router": "~13.3.0",
    "@angular/service-worker": "~13.3.0",
    "@ngneat/until-destroy": "^9.2.3",
    "@ngrx/effects": "^13.2.0",
    "@ngrx/store": "^13.2.0",
    "@ngrx/store-devtools": "^13.2.0",
    "bootstrap": "^5.2.3",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.10",
    "@angular-eslint/builder": "13.5.0",
    "@angular-eslint/eslint-plugin": "13.5.0",
    "@angular-eslint/eslint-plugin-template": "13.5.0",
    "@angular-eslint/schematics": "13.5.0",
    "@angular-eslint/template-parser": "13.5.0",
    "@angular/cli": "~13.3.10",
    "@angular/compiler-cli": "~13.3.0",
    "@nguniversal/builders": "^13.1.1",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "latest",
    "eslint-plugin-jsdoc": "latest",
    "eslint-plugin-prefer-arrow": "latest",
    "jasmine-core": "~4.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "prettier": "^2.8.4",
    "rimraf": "^3.0.2",
    "typescript": "~4.6.2"
  }
}
