{
  "name": "hpzone",
  "author": "anirbmuk",
  "description": "Angular 10 application for managing my HP gas delivery schedule and status",
  "version": "4.0.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anirbanstore/hpzone.git"
  },
  "bugs": {
    "url": "https://github.com/anirbanstore/hpzone/issues"
  },
  "homepage": "https://anirbanstore.github.io/hpzone/",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "test-ci": "ng test --watch=false",
    "build-prod": "ng build --aot --prod",
    "pretty-check": "prettier --config ./.prettierrc --list-different \"src/app/**/*{.ts,.json,.css,.html}\"",
    "pretty-fix": "prettier --config ./.prettierrc --write \"src/app/**/*{.ts,.json,.css,.html}\"",
    "deploy": "npm run test-ci && npm run lint && npm run pretty-check && npm run build-prod && firebase deploy"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.2.4",
    "@angular/common": "~10.2.4",
    "@angular/compiler": "~10.2.4",
    "@angular/core": "~10.2.4",
    "@angular/forms": "~10.2.4",
    "@angular/platform-browser": "~10.2.4",
    "@angular/platform-browser-dynamic": "~10.2.4",
    "@angular/router": "~10.2.4",
    "@angular/service-worker": "~10.2.4",
    "@ngrx/effects": "^8.6.0",
    "@ngrx/store": "^8.6.0",
    "@ngrx/store-devtools": "^10.0.0",
    "bootstrap": "^3.4.1",
    "ngx-spinner": "^10.0.1",
    "rxjs": "~6.6.3",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1002.1",
    "@angular/cli": "~10.2.1",
    "@angular/compiler-cli": "~10.2.4",
    "@angular/language-service": "~10.2.4",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "angular-cli-ghpages": "^0.6.2",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "prettier": "^2.2.1",
    "protractor": "~7.0.0",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.0",
    "tslint-etc": "^1.13.9",
    "typescript": "~4.0.5"
  }
}
